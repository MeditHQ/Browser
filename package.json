{
  "name": "ohhaibrowser",
  "version": "3.3.0",
  "license": "ISC",
  "description": "Browser the internet",
  "homepage": "https://ohhaibrowser.com",
  "author": {
    "name": "Lucas Reade",
    "email": "lucas.reade@gmail.com"
  },
  "main": "main.js",
  "scripts": {
    "build": "webpack --mode=development",
    "release": "webpack --mode=production",
    "lint": "eslint -c .eslintrc.json . --ext .js --fix",
    "debug": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "files": [
    "*.js",
    "build",
    "node_modules"
  ],
  "build": {
    "icon": "browser/assets/imgs/frame/icon.png",
    "appId": "OhHai.Browser",
    "mac": {
      "icon": "browser/assets/imgs/frame/mac_icon.icns",
      "category": "public.app-category.utilities",
      "target": [
        "default"
      ]
    },
    "linux": {
      "icon": "browser/assets/imgs/frame/linux_icon.png",
      "target": [
        "deb"
      ]
    },
    "win": {
      "icon": "browser/assets/imgs/frame/win_icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "fileAssociations": {
      "ext": [
        "html",
        "htm"
      ],
      "name": "Web files",
      "role": "Viewer"
    },
    "protocols": {
      "name": "Web urls",
      "schemes": [
        "http",
        "https"
      ],
      "role": "Viewer"
    },
    "nsis": {
      "perMachine": "true"
    }
  },
  "dependencies": {
    "dexie": "^2.0.4"
  },
  "devDependencies": {
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.4.2",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "eslint": "^6.8.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  }
}
