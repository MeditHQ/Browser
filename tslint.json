{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [false],
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "max-line-length": [
            true,
            {
              "limit": 200,
              "ignore-pattern": "^import |^export {(.*?)}",
              "check-strings": true,
              "check-regex": true
            }
        ],
        "prefer-for-of": false
    },
    "rulesDirectory": []
}